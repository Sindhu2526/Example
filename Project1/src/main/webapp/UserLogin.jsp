<body>



	<h2 ALIGN="CENTER">User Login form</h2>







	<form action="Welcome.html" method="post">



		<table border="0" align="center">



			<tbody>







				<tr>



					<td>User Name:</td>



					<td><input id="username" maxlength="50" name="name"
						type="text" required /></td>



				</tr>















				<tr>



					<td>Password:</td>



					<td><input id="password" maxlength="50" name="password"
						type="password" required /></td>



				</tr>







				<tr>



					<td align="right"><input name="Submit" type="Submit"
						value="Submit" /></td>



				</tr>







			</tbody>



		</table>



	</form>







</body>



